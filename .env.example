# Fabric Docker Configuration
# Copy this file to .env and fill in your values

# ===================
# Server Ports
# ===================
PORT=8180                    # REST API port
WEB_PORT=5173               # Web UI port
MCP_PORT=8181               # MCP Server port

# ===================
# API Authentication
# ===================
API_KEY=                    # Optional: API key for REST API authentication

# ===================
# AI Provider Keys
# ===================
OPENAI_API_KEY=             # OpenAI API key
ANTHROPIC_API_KEY=          # Anthropic API key
GEMINI_API_KEY=             # Google Gemini API key
OLLAMA_HOST=                # Ollama host URL (e.g., http://host.docker.internal:11434)

# ===================
# Default Settings
# ===================
DEFAULT_MODEL=gpt-4o        # Default AI model
DEFAULT_VENDOR=openai       # Default AI vendor

# ===================
# System
# ===================
TZ=Asia/Shanghai            # Timezone
